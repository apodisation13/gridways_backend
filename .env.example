# порт для приложения наружу, типа localhost:8002/docs, localhost:8002/admin
APP_PORT_OUTSIDE=8002

# только при разработке через докер! для локального запуска - убрать, там есть дефолтное значение
KAFKA_BOOTSTRAP_SERVERS=kafka:29092

# пароль от базы для локальных тестов, ее надо создать руками тоже у себя
TEST_DB_PASSWORD=

EMAIL_PASSWORD=

TG_TOKEN=
TG_CHAT_ID=

