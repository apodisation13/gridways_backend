CONFIG=development_local

# параметры локальной базы данных, создать ее надо руками у себя
DB_USER=
DB_PASSWORD=
DB_HOST=localhost
DB_PORT=5432
DB_NAME=

# порт постгрес, который будет работать наружу, только при разработке через докер!
